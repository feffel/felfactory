{
    "name": "feffel/felfactory",
    "type": "library",
    "description": "",
    "keywords": [
        "feffel",
        "felfactory"
    ],
    "homepage": "https://github.com/feffel/felfactory",
    "license": "MIT",
    "authors": [
        {
            "name": "felfel",
            "email": "a.3lyounis@gmail.com",
            "homepage": "https://github.com/feffel",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "feffel\\felfactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "feffel\\felfactory\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
