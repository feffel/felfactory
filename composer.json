{
    "name": "feffel/felfactory",
    "type": "library",
    "description": "",
    "keywords": ["felfactory", "faker", "generation", "data", "fixtures"],
    "homepage": "https://github.com/feffel/felfactory",
    "license": "MIT",
    "authors": [
        {
            "name": "felfel",
            "email": "a.3lyounis@gmail.com",
            "homepage": "https://github.com/feffel",
            "role": "Developer"
        }
    ],
    "repositories": {
        "php-di\\phpdoc-reader": {
            "type": "vcs",
            "url": "https://github.com/feffel/PhpDocReader",
            "no-api" : true
        }
    },
    "require": {
        "php": "~7.1",
        "fzaninotto/faker": "^1.8",
        "haydenpierce/class-finder": "^0.3.1",
        "php-di/phpdoc-reader": "dev-master",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": ">=7.0",
        "psy/psysh": "^0.9.9",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^4.2",
        "vimeo/psalm": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "felfactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "felfactory\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
